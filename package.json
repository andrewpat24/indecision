{
  "name": "indecision-app",
  "version": "1.0.0",
  "description": "A client side webapp to solve user indecision",
  "main": "server.js",
  "scripts": {
    "server": "live-server public",
    "build": "webpack --watch",
    "build-babel": "babel src/app.js --out-file=public/scripts/app.js --presets=env,react --watch",
    "dev": "concurrently \"npm run server\" \"npm run build\" ",
    "prototype": "concurrently \"live-server public\" \"babel src/playground/prototype.js --out-file=public/scripts/app.js --presets=env,react --watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewpat24/indecision.git"
  },
  "author": "Andrew Patterson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewpat24/indecision/issues"
  },
  "homepage": "https://github.com/andrewpat24/indecision#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "live-server": "^1.2.1",
    "webpack": "^3.1.0"
  }
}
